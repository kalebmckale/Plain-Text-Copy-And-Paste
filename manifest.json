{
	"name": "Plain Text Copy And Paste",
	"version": "1.0.1",
	"description": "Removes formatting from copied text and clears the selection after copying.",
	"content_scripts": [
		{
	  		"matches": ["<all_urls>"],
	  		"js": ["script.js"],
	  		"run_at": "document_end",
	  		"all_frames": true
		}
	],
	"icons": { 
		"16": "16.png",
		"48": "48.png",
		"128": "128.png" 
	},
	"minimum_chrome_version": "16.0.0.0"
}